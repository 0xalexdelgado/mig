{
    "Name":     "policy_system_auditd_low",
    "Target":   "linux",
    "Check":    "filechecker",
    "Arguments": {
        "auditd_execve": {
            "Path": "/etc/audit/audit.rules",
            "Type": "contains",
            "Value": "-a exit,always -F arch=b(32|64) -S execve -k exec"
        }
    },
    "RunDate":  "immediate",
    "Expiration":  "30m"
}
